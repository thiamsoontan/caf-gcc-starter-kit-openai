private_dns = {
  aks_dns_re1 = {
    name               = "privatelink.southeastasia.azmk8s.io"
    resource_group_key = "aks_re1"

    # vnet link is created after vnet is crated.
    vnet_links = {
      aks_internet_vnet_link = {
        name               = "aks-internet-vnet"
        registration_enabled = false # a virtual network can only link to one private dns zone with registration enabled
        virtual_network_id = "/subscriptions/xxxxxxxx-4066-42f0-b0fa-xxxxxxxxxxxx/resourceGroups/gcci-rg-agency-vnets/providers/Microsoft.Network/virtualNetworks/gcci-vnet-internet"
      }   
      aks_management_vnet_link = {
        name                 = "aks-management-vnet"
        registration_enabled = false
        virtual_network_id   = "/subscriptions/xxxxxxxx-4066-42f0-b0fa-xxxxxxxxxxxx/resourceGroups/gcci-rg-agency-vnets/providers/Microsoft.Network/virtualNetworks/gcci-vnet-management"
      }
      aks_devops_vnet_link = {
        name                 = "aks-devops-vnet"
        registration_enabled = false
        virtual_network_id   = "/subscriptions/xxxxxxxx-4066-42f0-b0fa-xxxxxxxxxxxx/resourceGroups/gcci-rg-agency-vnets/providers/Microsoft.Network/virtualNetworks/gcci-vnet-devops"
      }                          
    }

    tags = { 
      purpose = "internet private dns" 
      project-code = "{{project_code}}" 
      env = "{{caf_environment}}" 
      zone = "{{zone_code}}"
      tier = "{{tier_code}}"          
    } 

  }

}